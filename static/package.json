{
  "name": "eamon",
  "version": "0.1.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run build-css:w\" \"npm run browser-sync\" ",
    "browser-sync": "browser-sync start --files \"myapp/static/css/*.css\" --proxy 127.0.0.1:8000 --serveStatic: static",
    "lite": "lite-server",
    "test": "live-server --open=unit-tests.html",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "build-css": "node-sass scss -o css",
    "build-css:w": "node-sass -w scss -o css",
    "lint": "tslint app/**/*.ts"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common":  "2.0.0-rc.1",
    "@angular/compiler":  "2.0.0-rc.1",
    "@angular/core":  "2.0.0-rc.1",
    "@angular/http":  "2.0.0-rc.1",
    "@angular/platform-browser":  "2.0.0-rc.1",
    "@angular/platform-browser-dynamic":  "2.0.0-rc.1",
    "@angular/router":  "2.0.0-rc.1",
    "@angular/router-deprecated":  "2.0.0-rc.1",
    "@angular/upgrade":  "2.0.0-rc.1",
    "systemjs": "0.19.27",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12",
    "angular2-in-memory-web-api": "0.0.7",
    "bootstrap": "^3.3.6"
  },
  "devDependencies": {
    "concurrently": "*",
    "jasmine-core": "*",
    "lite-server": "*",
    "node-sass": "*",
    "typescript": "*"
  }
}
